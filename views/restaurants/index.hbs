<h1>{{title}}</h1>
<div class="info">
    <h3>List of all restaurants</h3>
    <a href="/restaurants/new">Create new restaurant</a>
    <ul>
        {{#each restaurants}}
        <li>
            <a href="/restaurants/{{ this._id}}">{{ this.name}}</a>
            |
            <a href="/restaurants/{{ this._id}}/delete">Delete</a>
        </li>
        {{/each }}
    </ul>
    <div id="map"></div>
</div>
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDt7xKPA7bOxEwA0P_Mw9_0X29OS2MQB8I"></script>

<script type="text/javascript" src="/javascripts/main.js"></script>


  